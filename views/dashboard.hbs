  <div class='col-4'>
    <div class='card mb-3'>
      <div class='row no-gutters'>
        <div class='d-flex align-items-center justify-content-center'>
          <div
            class='rounded border bg-danger d-flex align-items-center justify-content-center shadow-sm m-3'
            style='width: auto;height: auto;padding: 14px;'
          >
            <svg
              xmlns='http://www.w3.org/2000/svg'
              width='40'
              height='40'
              fill='white'
              class='bi bi-emoji-frown'
              viewBox='0 0 16 16'
            >
              <path
                d='M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z'
              ></path>
              <path
                d='M4.285 12.433a.5.5 0 0 0 .683-.183A3.498 3.498 0 0 1 8 10.5c1.295 0 2.426.703 3.032 1.75a.5.5 0 0 0 .866-.5A4.498 4.498 0 0 0 8 9.5a4.5 4.5 0 0 0-3.898 2.25.5.5 0 0 0 .183.683zM7 6.5C7 7.328 6.552 8 6 8s-1-.672-1-1.5S5.448 5 6 5s1 .672 1 1.5zm4 0c0 .828-.448 1.5-1 1.5s-1-.672-1-1.5S9.448 5 10 5s1 .672 1 1.5z'
              ></path>
            </svg>
          </div>

        </div>
        <div class='d-flex align-items-center justify-content-center'>
          <div class='card-body pl-2'>
            <h4 class='card-title text-strong'>Not implemented</h4>
            <p class='card-subtitle text-primary'>{{{notImplemented.length}}} services</p>

          </div>
        </div>
      </div>
    </div>
  </div>

</div>

<div class='row'>
  <div class='col-8'>
    <div class='card' style='width: 100%;'>
      <div class='card-header'>Services List</div>

      <div class='card-body pt-4'>
        <div class='table-responsive'>
          <table id='services-table-home' class='table table' style='width:100%'>
            <thead>
              <tr>
                <th>Host name</th>
                <th>Status</th>
                <th>Enabled</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              {{#each allServices.service_reports}}
                <tr>
                  <td><span class='text-primary'>{{this.host.name}}</span>
                    {{#if this.total_alerts}}
                      <button
                        type='button'
                        tabindex='0'
                        data-toggle='popover'
                        data-trigger='focus'
                        class='btn btn-outline-info btn-sm ml-2 py-1 px-2'
                        data-placement='top'
                        data-container='body'
                        data-content='{{this.nodes.0.status_text}}'
                      >{{this.total_alerts}} alert</button>{{/if}}
                  </td>
                  <td><h5>{{{status this.status_text}}}</h5></td>
                  <td>{{{toggle this.host.enabled}}}</td>
                  <td class='text-center'>
                    <a href='service/{{this.host.id}}' type='button' class='btn btn-info px-2'>
                      <svg
                        xmlns='http://www.w3.org/2000/svg'
                        width='16'
                        height='16'
                        fill='currentColor'
                        class='bi bi-eye-fill'
                        viewBox='0 0 16 16'
                      >
                        <path d='M10.5 8a2.5 2.5 0 1 1-5 0 2.5 2.5 0 0 1 5 0z'></path>
                        <path
                          d='M0 8s3-5.5 8-5.5S16 8 16 8s-3 5.5-8 5.5S0 8 0 8zm8 3.5a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7z'
                        ></path>
                      </svg>
                    </a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
      <div class='card-footer'>

        <a href='/services/' class='btn btn-primary card-link float-right'>View detailed list</a>
      </div>
    </div>
  </div>
  <div class='col-4'>
    <div class='card border-default'>
      <div class='card-header'>Services Statuses</div>
      <div class='card-body'>
        <canvas id='myChart' width='400' height='400'></canvas>
      </div>
      <div class='card-footer'>
        <a href='/services/statuses' class='btn btn-info btn-block card-link float-right'>View
          Grouped Services</a>

      </div>
    </div>
  </div>
</div>